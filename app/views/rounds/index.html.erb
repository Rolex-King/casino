<p id="notice"><%= notice %></p>

<h1>Listado de rondas</h1>

<table>
  <thead>
    <tr>
      <th>Jugador</th>
      <th>Resultado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rounds.each do |round| %>
      <tr>
        <td><%= round.user.email %></td>
        <td><%= round.resultado %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Crear ronda', new_round_path %>
